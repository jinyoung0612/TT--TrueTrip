    <script>
        $(document).ready(function () {
            var content;
            var User_ID = $('#UserID').text();
            var postsArr = ""

            var posting = $.post('/showPlannersImage', {'UserID': User_ID})

            posting.done(function (data) {
                content = data.data

                for (var i = 0; i < content.length; i++) {
                    postsArr +=
                        "<div class=\"col-lg-4 col-sm-6\" >" +
                        "                            <a class=\"arrange-items\" href=\"single-listing.html\">" +
                        "                            <div class=\"arrange-pic\">" +
                        "                            <img class=\"postpic\" src=\'" + content[i].path + "' style='width:264px; height:223px position:fixed' alt=\"\">" +
                        "                            </div>" +
                        "                            <div class=\"arrange-text\">" +
                        "                            <h5>" + content[i].title + "</h5>" +
                        "                        </div>" +
                        "                       </a>" +
                        "                     </div>"
                }
                $('#showPlannerImages').html(postsArr)
            })
        })

